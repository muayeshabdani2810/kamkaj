<script>
    $(window).unload(function () {
        var url = window.location.href;
        var Name = $('#JobName').val();
        var Phone = $('#JobMobileNumber').val();
        if (Name !== "" && (Phone !== "" && Phone.length == 11)) {
            var Url = "/SendBeacon/SaveUserInfoOnUnloadForm";
            var data = {
                Name: $('#JobName').val(),
                PhoneNumber: $('#JobMobileNumber').val(),
                Service: $('#JobProgramId').val(),
                City: $('#JobCity').val(),
                Area: $('#JobArea').val(),
                Address: $('#JobAddress').val(),
                Url: url
            };

            let headers = {
                type: 'application/json'
            };
            let blob = new Blob([JSON.stringify(data)], headers);
            navigator.sendBeacon(Url, blob);
        }
    });
</script>

<script>
    $(document).on("ready", function () {
        getUserData();
        getCampaignInfo();
        $(".imgsec-VideoId").on("click", function (e) {
            var $VideoId = e.currentTarget.id
            $(this).html('');
            $("#" + $VideoId).show();
            $("#" + $VideoId).html(`<iframe style="height:270px; width:100%; border-radius:10px" type="text/html"
      src="https://www.youtube.com/embed/`+ $VideoId + `?autoplay=1;wmode=opaque&amp;rel=0&amp;autohide=1&amp;showinfo=0&amp;wmode=transparent"
      frameborder="0" allowfullscreen="1" showinfo="1">
      </iframe>`);
        });
    });
    function getUserData() {
        var obj = getDataFromLocalStorage();
        if (obj !== null) {
            $("#JobName").val(obj.Name);
            $("#JobMobile").val(obj.MobileNumber);
            $("#JobMobileNumber").val(obj.MobileNumber);
            if (obj.City) {
                const city = obj.City.charAt(0).toUpperCase() + obj.City.slice(1);
                $("#JobCity1").val(city);
                onCityChange();
                if (obj.Area) {
                    $("#JobArea").data("kendoComboBox").value(obj.Area);
                }
            }
            $("#JobAddress").val(obj.Address);
        }
    };
    function getCampaignInfo() {
        var campaign = getCampaignInfoFromLocalStorage();
        if (campaign !== null) {
            $("#LeadCampaignName").val(campaign.CampaignName);
            $("#HdnPlatformName").val(campaign.Platform);
        }
    }

</script>

<script>
    $('.notification-modal').modal({ backdrop: 'static', keyboard: false })
    ////// FormCity option  Selection
    function cityselection() {
        var cityInput = document.querySelector('#City').value;
        var formCityselection = document.querySelector('#JobCity1');
        let cityLenght = formCityselection.length;
        for (var i = 0; i < cityLenght; i++) {
            if (formCityselection.options[i].value === cityInput) {
                formCityselection.options[i].selected = true;
                formCityselection.options[i].value = cityInput
            }
        }
    }

</script>

